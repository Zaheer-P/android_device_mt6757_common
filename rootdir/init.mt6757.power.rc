on boot
# Performance Profiles
# Power save
on property:sys.perf.profile=0
    write /sys/devices/system/cpu/cpu0/cpufreq/scaling_max_freq "871000"
    write /sys/devices/system/cpu/cpu1/cpufreq/scaling_max_freq "871000"
    write /sys/devices/system/cpu/cpu2/cpufreq/scaling_max_freq "871000"
    write /sys/devices/system/cpu/cpu3/cpufreq/scaling_max_freq "871000"
    write /sys/devices/system/cpu/cpu0/cpufreq/scaling_min_freq "286000"
    write /sys/devices/system/cpu/cpu1/cpufreq/scaling_min_freq "286000"
    write /sys/devices/system/cpu/cpu2/cpufreq/scaling_min_freq "286000"
    write /sys/devices/system/cpu/cpu3/cpufreq/scaling_min_freq "286000"
    write /sys/devices/system/cpu/cpu0/cpufreq/scaling_governor "interactive"
    write /sys/devices/system/cpu/cpu1/cpufreq/scaling_governor "interactive"
    write /sys/devices/system/cpu/cpu2/cpufreq/scaling_governor "interactive"
    write /sys/devices/system/cpu/cpu3/cpufreq/scaling_governor "interactive"

# Balanced
on property:sys.perf.profile=1
    write /sys/devices/system/cpu/cpu0/cpufreq/scaling_max_freq "2152000"
    write /sys/devices/system/cpu/cpu1/cpufreq/scaling_max_freq "2152000"
    write /sys/devices/system/cpu/cpu2/cpufreq/scaling_max_freq "2152000"
    write /sys/devices/system/cpu/cpu3/cpufreq/scaling_max_freq "2152000"   
    write /sys/devices/system/cpu/cpu0/cpufreq/scaling_min_freq "286000"
    write /sys/devices/system/cpu/cpu1/cpufreq/scaling_min_freq "286000"
    write /sys/devices/system/cpu/cpu2/cpufreq/scaling_min_freq "286000"
    write /sys/devices/system/cpu/cpu3/cpufreq/scaling_min_freq "286000"
    write /sys/devices/system/cpu/cpu0/cpufreq/scaling_governor "interactive"
    write /sys/devices/system/cpu/cpu1/cpufreq/scaling_governor "interactive"
    write /sys/devices/system/cpu/cpu2/cpufreq/scaling_governor "interactive"
    write /sys/devices/system/cpu/cpu3/cpufreq/scaling_governor "interactive"
    
# High performance
on property:sys.perf.profile=2
    write /sys/devices/system/cpu/cpu0/cpufreq/scaling_max_freq "2508000"
    write /sys/devices/system/cpu/cpu1/cpufreq/scaling_max_freq "2508000"
    write /sys/devices/system/cpu/cpu2/cpufreq/scaling_max_freq "2508000"
    write /sys/devices/system/cpu/cpu3/cpufreq/scaling_max_freq "2508000"
    write /sys/devices/system/cpu/cpu0/cpufreq/scaling_min_freq "663000"
    write /sys/devices/system/cpu/cpu1/cpufreq/scaling_min_freq "663000"
    write /sys/devices/system/cpu/cpu2/cpufreq/scaling_min_freq "663000"
    write /sys/devices/system/cpu/cpu3/cpufreq/scaling_min_freq "663000"
    write /sys/devices/system/cpu/cpu0/cpufreq/scaling_governor "interactive"
    write /sys/devices/system/cpu/cpu1/cpufreq/scaling_governor "interactive"
    write /sys/devices/system/cpu/cpu2/cpufreq/scaling_governor "interactive"
    write /sys/devices/system/cpu/cpu3/cpufreq/scaling_governor "interactive"

# Bias power save
on property:sys.perf.profile=3
    write /sys/devices/system/cpu/cpu0/cpufreq/scaling_max_freq "1996000"
    write /sys/devices/system/cpu/cpu1/cpufreq/scaling_max_freq "1996000"
    write /sys/devices/system/cpu/cpu2/cpufreq/scaling_max_freq "1996000"
    write /sys/devices/system/cpu/cpu3/cpufreq/scaling_max_freq "1996000"   
    write /sys/devices/system/cpu/cpu0/cpufreq/scaling_min_freq "286000"
    write /sys/devices/system/cpu/cpu1/cpufreq/scaling_min_freq "286000"
    write /sys/devices/system/cpu/cpu2/cpufreq/scaling_min_freq "286000"
    write /sys/devices/system/cpu/cpu3/cpufreq/scaling_min_freq "286000"  
    write /sys/devices/system/cpu/cpu0/cpufreq/scaling_governor "interactive"
    write /sys/devices/system/cpu/cpu1/cpufreq/scaling_governor "interactive"
    write /sys/devices/system/cpu/cpu2/cpufreq/scaling_governor "interactive"
    write /sys/devices/system/cpu/cpu3/cpufreq/scaling_governor "interactive"
